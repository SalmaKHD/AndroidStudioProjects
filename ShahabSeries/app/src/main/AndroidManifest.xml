        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ShahabSeries"
        tools:targetApi="31">
        <activity
            android:name=".instagram.MainInstagramActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.salmakhd.android.rxjava.RxJavaDemonstration"
            android:exported="false" />
        <activity
            android:name=".viewpager.ViewPagerActivity"
            android:exported="true">

        </activity>
        <activity
            android:name=".cityapplication.activities.HomeActivity"
            android:exported="false" />
        <activity
            android:name=".cityapplication.activities.CityActivity"
            android:exported="true" />
        <activity
            android:name=".navigationapplication.NavigationActivity"
            android:exported="true"
