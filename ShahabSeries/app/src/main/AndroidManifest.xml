        <service
            android:name=".navigationapplication.SenfiService"
            android:exported="false" />

        <activity
            android:name=".tictactoe.IntroActivity"
            android:exported="true" />
        <activity
            android:name=".tictactoe.TicTacToeActivity"
            android:exported="false" />
        <activity
            android:name=".projectone.FirstActivity"
